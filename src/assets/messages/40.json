          },
          "111230001": {
            "type": "Text",
            "id": 111230001,
            "sectionId": 1112300,
            "senderId": 40,
            "text": "It'd be better to speak face to face. I'll wait for you at the same old spot in the restricted zone",
            "option": null,
            "nextIds": [
              111230002,
              111230003
            ],
            "kind": "NPC"
          },
          "111230002": {
            "type": "Text",
            "id": 111230002,
