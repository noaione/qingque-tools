    [
      {
        "type": "Message",
        "id": 1217300,
        "startIds": [
          121730001
        ],
        "messages": {
          "121730001": {
            "type": "Text",
            "id": 121730001,
            "sectionId": 1217300,
            "senderId": 8000,
            "text": "Is everything okay, Dan Heng?",
            "option": null,
            "nextIds": [
              121730002
            ],
            "kind": "NPC"
          },
          "121730002": {
            "type": "Text",
            "id": 121730002,
            "sectionId": 1217300,
            "senderId": 1001,
            "text": "{NICKNAME} and I heard you received an anonymous letter, and we were worried, so we came here to check on you, but you weren't in the archives.",
            "option": null,
            "nextIds": [
              121730003
            ],
            "kind": "NPC"
          },
          "121730003": {
            "type": "Text",
            "id": 121730003,
            "sectionId": 1217300,
            "senderId": 1001,
            "text": "Did that poker-faced Stellaron Hunter \"Blade\" give you any trouble again?",
            "option": null,
            "nextIds": [
              121730004,
              121730005,
              121730006
            ],
            "kind": "NPC"
          },
          "121730004": {
            "type": "Text",
            "id": 121730004,
            "sectionId": 1217300,
            "senderId": 1002,
            "text": "Don't worry. I just stepped away to enjoy the view.",
            "option": "I'm okay...",
            "nextIds": [
              121730007
            ],
            "kind": "Player"
          },
          "121730007": {
            "type": "Text",
            "id": 121730007,
            "sectionId": 1217300,
            "senderId": 1001,
            "text": "Phew, that's a relief, then.",
            "option": null,
            "nextIds": [
              121730008
            ],
            "kind": "NPC"
          },
          "121730008": {
            "type": "Text",
            "id": 121730008,
            "sectionId": 1217300,
            "senderId": 1001,
            "text": "By the way, don't forget to head back to the Express early. Himeko said she made coffee for us.",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "121730005": {
            "type": "Text",
            "id": 121730005,
            "sectionId": 1217300,
            "senderId": 1002,
            "text": "Don't worry. I just stepped away for a bit to catch up with some old friends.",
            "option": "I went to meet some old friends.",
            "nextIds": [
              121730007
            ],
            "kind": "Player"
          },
          "121730006": {
            "type": "Text",
            "id": 121730006,
            "sectionId": 1217300,
            "senderId": 1002,
            "text": "Don't worry. I've got rid of that letter.",
            "option": "I've thrown away the letter.",
            "nextIds": [
              121730007
            ],
            "kind": "Player"
          }
        }
      }
    ],
    [
      {
        "type": "Message",
        "id": 1300100,
        "startIds": [
          130010000
        ],
        "messages": {
          "130010000": {
            "type": "Text",
            "id": 130010000,
            "sectionId": 1300100,
            "senderId": 1000,
            "text": "Hey, hey!",
            "option": null,
            "nextIds": [
              130010001
            ],
            "kind": "NPC"
          },
          "130010001": {
            "type": "Text",
            "id": 130010001,
            "sectionId": 1300100,
            "senderId": 1001,
            "text": "Hey, hey!",
            "option": null,
            "nextIds": [
              130010002
            ],
            "kind": "NPC"
          },
          "130010002": {
            "type": "Text",
            "id": 130010002,
            "sectionId": 1300100,
            "senderId": 1002,
            "text": "...",
            "option": null,
            "nextIds": [
              130010003,
              130010004
            ],
            "kind": "NPC"
          },
          "130010003": {
            "type": "Text",
            "id": 130010003,
            "sectionId": 1300100,
            "senderId": null,
            "text": "What's up?",
            "option": "What's up?",
            "nextIds": [
              130010006
            ],
            "kind": "Player"
          },
          "130010006": {
            "type": "Text",
            "id": 130010006,
            "sectionId": 1300100,
            "senderId": 1003,
            "text": "Are we ready to set off to the next stop?",
            "option": null,
            "nextIds": [
              130010007
            ],
            "kind": "NPC"
          },
          "130010007": {
            "type": "Text",
            "id": 130010007,
            "sectionId": 1300100,
            "senderId": 1000,
            "text": "We sure are! The warp jump device is all calibrated.",
            "option": null,
            "nextIds": [
              130010008
            ],
            "kind": "NPC"
          },
          "130010008": {
            "type": "Text",
            "id": 130010008,
            "sectionId": 1300100,
            "senderId": 1000,
            "text": "But, as the Express conductor, I have three things I wanted to stress before we depart.",
            "option": null,
            "nextIds": [
              130010009
            ],
            "kind": "NPC"
          },
          "130010009": {
            "type": "Text",
            "id": 130010009,
            "sectionId": 1300100,
            "senderId": 1000,
            "text": "That's why I'm here for this little Express meeting, too.",
            "option": null,
            "nextIds": [
              130010010
            ],
            "kind": "NPC"
          },
          "130010010": {
            "type": "Text",
            "id": 130010010,
            "sectionId": 1300100,
            "senderId": 1001,
            "text": "Alright! Then you know the rules.",
            "option": null,
            "nextIds": [
              130010011
            ],
            "kind": "NPC"
          },
          "130010011": {
            "type": "Text",
            "id": 130010011,
            "sectionId": 1300100,
            "senderId": 1001,
            "text": "We'll meet in the Parlor Car!",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "130010004": {
            "type": "Text",
            "id": 130010004,
            "sectionId": 1300100,
            "senderId": null,
            "text": "Hey, hey!",
            "option": "Hello.",
            "nextIds": [
              130010005
            ],
            "kind": "Player"
          },
          "130010005": {
            "type": "Text",
            "id": 130010005,
            "sectionId": 1300100,
            "senderId": 1004,
            "text": "Alright, let's see what the conductor has to say first.",
            "option": null,
            "nextIds": [
              130010006
            ],
            "kind": "NPC"
          }
        }
      }
    ],
    [
      {
        "type": "Message",
        "id": 1300300,
        "startIds": [
          130030000
        ],
        "messages": {
          "130030000": {
            "type": "Text",
            "id": 130030000,
            "sectionId": 1300300,
            "senderId": null,
            "text": "Himeko invited {NICKNAME}, March, and Welt to join the group chat",
            "option": null,
            "nextIds": [
              130030001
            ],
            "kind": "System"
          },
          "130030001": {
            "type": "Text",
            "id": 130030001,
            "sectionId": 1300300,
            "senderId": 1003,
            "text": "Are we all having fun?",
            "option": null,
            "nextIds": [
              130030002
            ],
            "kind": "NPC"
          },
          "130030002": {
            "type": "Text",
            "id": 130030002,
            "sectionId": 1300300,
            "senderId": 1001,
            "text": "Lots of fun, sure",
            "option": null,
            "nextIds": [
              130030003
            ],
            "kind": "NPC"
          },
          "130030003": {
            "type": "Text",
            "id": 130030003,
            "sectionId": 1300300,
            "senderId": 1001,
            "text": "But something feels off...",
            "option": null,
            "nextIds": [
              130030004
            ],
            "kind": "NPC"
          },
          "130030004": {
            "type": "Text",
            "id": 130030004,
            "sectionId": 1300300,
            "senderId": 1001,
            "text": "It doesn't feel very real. It's odd. I can't describe it",
            "option": null,
            "nextIds": [
              130030005,
              130030006
            ],
            "kind": "NPC"
          },
          "130030005": {
            "type": "Text",
            "id": 130030005,
            "sectionId": 1300300,
            "senderId": null,
            "text": "We're in a dream, after all",
            "option": "We're in a dream, after all",
            "nextIds": [
              130030007
            ],
            "kind": "Player"
          },
          "130030007": {
            "type": "Text",
            "id": 130030007,
            "sectionId": 1300300,
            "senderId": 1001,
            "text": "I saw a guy wearing a plaster head sculpture at the auction, and he walked off without doing anything. That's pretty suspicious",
            "option": null,
            "nextIds": [
              130030008
            ],
            "kind": "NPC"
          },
          "130030008": {
            "type": "Text",
            "id": 130030008,
            "sectionId": 1300300,
            "senderId": 1004,
            "text": "I also had some unpleasant discoveries. It's a bit complicated. It's best if we meet up face-to-face.",
            "option": null,
            "nextIds": [
              130030009
            ],
            "kind": "NPC"
          },
          "130030009": {
            "type": "Text",
            "id": 130030009,
            "sectionId": 1300300,
            "senderId": 1003,
            "text": "Me too",
            "option": null,
            "nextIds": [
              130030010
            ],
            "kind": "NPC"
          },
          "130030010": {
            "type": "Text",
            "id": 130030010,
            "sectionId": 1300300,
