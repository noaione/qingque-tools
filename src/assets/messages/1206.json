          },
          "200630009": {
            "type": "Sticker",
            "id": 200630009,
            "sectionId": 2006300,
            "senderId": null,
            "text": "[confused]",
            "option": null,
            "nextIds": [
              200630010
            ],
            "kind": "PlayerAuto",
            "sticker": {
              "id": 20007,
              "path": "icon/emoji/20007.png",
              "keywords": "Puzzled"
            }
          },
          "200630010": {
            "type": "Text",
            "id": 200630010,
            "sectionId": 2006300,
            "senderId": 1206,
            "text": "Oh, wouldn't you know!",
            "option": null,
            "nextIds": [
              200630011
            ],
            "kind": "NPC"
          },
          "200630011": {
            "type": "Text",
            "id": 200630011,
            "sectionId": 2006300,
            "senderId": 1206,
            "text": "When I was still studying, he barely ever acknowledged me.",
