            }
          },
          "200630029": {
            "type": "Text",
            "id": 200630029,
            "sectionId": 2006300,
            "senderId": 1206,
            "text": "*sigh* I hope I won't run into him ever again...",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "200630012": {
            "type": "Text",
            "id": 200630012,
            "sectionId": 2006300,
            "senderId": null,
            "text": "What's a lyceum? And what's abecedary?",
            "option": "Lyceum? Abecedary?",
            "nextIds": [
              200630013
            ],
            "kind": "Player"
          },
          "200630013": {
            "type": "Text",
            "id": 200630013,
            "sectionId": 2006300,
            "senderId": null,
            "text": "I'm an alien, I don't know these words",
            "option": null,
