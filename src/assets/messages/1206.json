        "messages": {
          "200620000": {
            "type": "Text",
            "id": 200620000,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "{NICKNAME}",
            "option": null,
            "nextIds": [
              200620001
            ],
            "kind": "NPC"
          },
          "200620001": {
            "type": "Text",
            "id": 200620001,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "Do you know Yanqing?",
            "option": null,
            "nextIds": [
              200620002,
              200620024,
              200620006
            ],
            "kind": "NPC"
          },
          "200620002": {
            "type": "Text",
            "id": 200620002,
            "sectionId": 2006200,
            "senderId": null,
            "text": "I have heard of his name",
            "option": "I've heard of him",
            "nextIds": [
              200620003
            ],
            "kind": "Player"
          },
          "200620003": {
            "type": "Text",
            "id": 200620003,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "He's the golden child of the Cloud Knights",
            "option": null,
            "nextIds": [
              200620012
            ],
            "kind": "NPC"
          },
          "200620012": {
            "type": "Text",
            "id": 200620012,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "I witnessed his sword practice today. I can't even follow his movement with my eyes! I'm so shocked",
            "option": null,
            "nextIds": [
              200620013
            ],
            "kind": "NPC"
          },
          "200620013": {
            "type": "Text",
            "id": 200620013,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "He's so young! How come he's already so impressive",
            "option": null,
            "nextIds": [
              200620014,
              200620017
            ],
            "kind": "NPC"
          },
          "200620014": {
            "type": "Text",
            "id": 200620014,
            "sectionId": 2006200,
            "senderId": null,
            "text": "Could he be one of those martial prodigies?",
            "option": "Is he a prodigy",
            "nextIds": [
              200620015
            ],
            "kind": "Player"
          },
          "200620015": {
            "type": "Text",
            "id": 200620015,
            "sectionId": 2006200,
            "senderId": null,
            "text": "Maybe he just has the physical affinity for battle",
            "option": null,
            "nextIds": [
              200620016
            ],
            "kind": "PlayerAuto"
          },
          "200620016": {
            "type": "Text",
            "id": 200620016,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "That sounds right, yeah!",
            "option": null,
            "nextIds": [
              200620020
            ],
            "kind": "NPC"
          },
          "200620020": {
            "type": "Text",
            "id": 200620020,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "But now that I've thought about it, the way he wields his sword kinda feels similar to how my mom does it",
            "option": null,
            "nextIds": [
              200620021
            ],
            "kind": "NPC"
          },
          "200620021": {
            "type": "Text",
            "id": 200620021,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "I should ask him for tips",
            "option": null,
            "nextIds": [
              200620022
            ],
            "kind": "NPC"
          },
          "200620022": {
            "type": "Text",
            "id": 200620022,
            "sectionId": 2006200,
            "senderId": null,
            "text": "I'm willing to accompany you!",
            "option": "I'm willing to accompany you!",
            "nextIds": [
              200620023
            ],
            "kind": "Player"
          },
          "200620023": {
            "type": "Text",
            "id": 200620023,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "Alright!",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "200620017": {
            "type": "Text",
            "id": 200620017,
            "sectionId": 2006200,
            "senderId": null,
            "text": "Could it be due to General Jing Yuan's personal coaching",
            "option": "Is it because of Jing Yuan's teaching",
            "nextIds": [
              200620018
            ],
            "kind": "Player"
          },
          "200620018": {
            "type": "Text",
            "id": 200620018,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "I guess that's what it means by \"standing on the shoulder of giants\"",
            "option": null,
            "nextIds": [
              200620019
            ],
            "kind": "NPC"
          },
          "200620019": {
            "type": "Text",
            "id": 200620019,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "My mom's a Cloud Knight too, and she forced me to practice since I was young, so why am I not as good as him...?",
            "option": null,
            "nextIds": [
              200620020
            ],
            "kind": "NPC"
          },
          "200620024": {
            "type": "Text",
            "id": 200620024,
            "sectionId": 2006200,
            "senderId": null,
            "text": "We traded blows a few times",
            "option": "We've traded blows",
            "nextIds": [
              200620025
            ],
            "kind": "Player"
          },
          "200620025": {
            "type": "Text",
            "id": 200620025,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "What? You fought with the golden child of the Cloud Knights?",
            "option": null,
            "nextIds": [
              200620026
            ],
            "kind": "NPC"
          },
          "200620026": {
            "type": "Text",
            "id": 200620026,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "I don't believe you...",
            "option": null,
            "nextIds": [
              200620012
            ],
            "kind": "NPC"
          },
          "200620006": {
            "type": "Text",
            "id": 200620006,
            "sectionId": 2006200,
            "senderId": null,
            "text": "Who's Yanqing...",
            "option": "Who's Yanqing",
            "nextIds": [
              200620007
            ],
            "kind": "Player"
          },
          "200620007": {
            "type": "Sticker",
            "id": 200620007,
            "sectionId": 2006200,
            "senderId": null,
            "text": "[Confused]",
            "option": null,
            "nextIds": [
              200620008
            ],
            "kind": "PlayerAuto",
            "sticker": {
              "id": 20007,
              "path": "icon/emoji/20007.png",
              "keywords": "Puzzled"
            }
          },
          "200620008": {
            "type": "Text",
            "id": 200620008,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "You must know him",
            "option": null,
            "nextIds": [
              200620009
            ],
            "kind": "NPC"
          },
          "200620009": {
            "type": "Text",
            "id": 200620009,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "It's that kid following General Jing Yuan around",
            "option": null,
            "nextIds": [
              200620010
            ],
            "kind": "NPC"
          },
          "200620010": {
            "type": "Text",
            "id": 200620010,
            "sectionId": 2006200,
            "senderId": null,
            "text": "The general's guard?",
            "option": "The general's guard?",
            "nextIds": [
              200620011
            ],
            "kind": "Player"
          },
          "200620011": {
            "type": "Text",
            "id": 200620011,
            "sectionId": 2006200,
            "senderId": 1206,
            "text": "That's him. He's the golden child of the Cloud Knights!",
            "option": null,
            "nextIds": [
              200620012
            ],
            "kind": "NPC"
          }
        }
      }
