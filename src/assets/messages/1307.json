          },
          "130420017": {
            "type": "Text",
            "id": 130420017,
            "sectionId": 1304200,
            "senderId": 1307,
            "text": "I can't be the one doing all the talking. By your logic, you should also have to prove that you are not Sparkle, {NICKNAME}.",
            "option": null,
            "nextIds": [
              130420018
            ],
            "kind": "NPC"
          },
          "130420018": {
