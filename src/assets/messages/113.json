          "122220018": {
            "type": "Text",
            "id": 122220018,
            "sectionId": 1222200,
            "senderId": null,
            "text": "So you want to come back and visit them?",
            "option": "Wanna come back and visit them?",
            "nextIds": [
              122220019
            ],
            "kind": "Player"
          },
          "122220006": {
            "type": "Text",
            "id": 122220006,
            "sectionId": 1222200,
            "senderId": null,
            "text": "Well, well, look who we have here! I was just thinking about having a chat with Madam Ruan Mei, and lo and behold, I caught up with her!",
            "option": "What a chance encounter!",
            "nextIds": [
              122220007
