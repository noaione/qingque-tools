            "nextIds": [
              201060022,
              201060028
            ],
            "kind": "NPC"
          },
          "201060022": {
            "type": "Text",
            "id": 201060022,
            "sectionId": 2010600,
            "senderId": null,
            "text": "Okay, okay, stop! Now this is getting embarrassing...",
            "option": "Stop! This is getting embarrassing.",
            "nextIds": [
              201060023
            ],
            "kind": "Player"
          },
          "201060023": {
            "type": "Text",
            "id": 201060023,
            "sectionId": 2010600,
            "senderId": 1302,
            "text": "Trust me. It's not embarrassing once you get used to it. Please come to me often and let me shower you with compliments, {NICKNAME}.",
            "option": null,
            "nextIds": [
              201060024
            ],
            "kind": "NPC"
          },
          "201060024": {
            "type": "Text",
            "id": 201060024,
            "sectionId": 2010600,
            "senderId": 1302,
            "text": "Because compliments lift spirits and bring out beauty.",
