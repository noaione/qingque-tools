            "nextIds": [
              201060032
            ],
            "kind": "PlayerAuto"
          },
          "201060032": {
            "type": "Text",
            "id": 201060032,
            "sectionId": 2010600,
            "senderId": null,
            "text": "You're gracefully endowed with innate valor, unyielding resilience, profound worth, untapped potential, unwavering disposition, and boundless happiness.",
            "option": null,
            "nextIds": [
              201060033
            ],
            "kind": "PlayerAuto"
          },
          "201060033": {
