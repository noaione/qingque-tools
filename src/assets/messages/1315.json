        "messages": {
          "201410000": {
            "type": "Text",
            "id": 201410000,
            "sectionId": 2014100,
            "senderId": 1315,
            "text": "Hey, {NICKNAME}. I rustled up some goods, you interested?",
            "option": null,
            "nextIds": [
              201410001
            ],
            "kind": "NPC"
          },
          "201410001": {
            "type": "Text",
            "id": 201410001,
            "sectionId": 2014100,
            "senderId": null,
            "text": "Voice message converted to text",
            "option": null,
            "nextIds": [
              201410002
            ],
            "kind": "System"
          },
          "201410002": {
            "type": "Text",
            "id": 201410002,
            "sectionId": 2014100,
            "senderId": 1315,
            "text": "Lemme tell ya, it's mighty fine, lit as fudge. Just crackin' open the pack, the scent hits yer noggin straight away. Then ya start droolin' like a hound, ya catch my drift. Just a whiff's enough to make ya giddy on cloud nine, taste it, and you're shootin' straight to the moon, hahahaha.",
            "option": null,
            "nextIds": [
              201410003
            ],
            "kind": "NPC"
          },
          "201410003": {
            "type": "Text",
            "id": 201410003,
            "sectionId": 2014100,
            "senderId": null,
            "text": "Voice message converted to text",
            "option": null,
            "nextIds": [
              201410004
            ],
            "kind": "System"
          },
          "201410004": {
            "type": "Text",
            "id": 201410004,
            "sectionId": 2014100,
            "senderId": null,
            "text": "Sounds fishy. What sorta stuff is that?",
            "option": "What sorta stuff is that?",
            "nextIds": [
              201410005
            ],
            "kind": "Player"
          },
          "201410005": {
            "type": "Text",
            "id": 201410005,
            "sectionId": 2014100,
            "senderId": null,
            "text": "Where did you get it from?",
            "option": null,
            "nextIds": [
              201410006
            ],
            "kind": "PlayerAuto"
          },
          "201410006": {
            "type": "Text",
            "id": 201410006,
