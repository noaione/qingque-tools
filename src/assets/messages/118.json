            "nextIds": [
              130320009
            ],
            "kind": "NPC"
          },
          "130320009": {
            "type": "Text",
            "id": 130320009,
            "sectionId": 1303200,
            "senderId": 118,
            "text": "Hurry to the hotel in the Dreamscape. I'll be waiting for you there",
            "option": null,
            "nextIds": [],
