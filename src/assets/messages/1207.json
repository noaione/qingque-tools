  "sections": [
    [
      {
        "type": "Message",
        "id": 2006700,
        "startIds": [
          200670000
        ],
        "messages": {
          "200670000": {
            "type": "Text",
            "id": 200670000,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "I hope it's ok to ask, {NICKNAME}",
            "option": null,
            "nextIds": [
              200670001
            ],
            "kind": "NPC"
          },
          "200670001": {
            "type": "Text",
            "id": 200670001,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "Do you actually like what you do? Does it align with your dreams?",
            "option": null,
            "nextIds": [
              200670002,
              200670003,
              200670004
            ],
            "kind": "NPC"
          },
          "200670002": {
            "type": "Text",
            "id": 200670002,
            "sectionId": 2006700,
            "senderId": null,
            "text": "Pretty similar, I quite like my situation right now",
            "option": "Similar",
            "nextIds": [
              200670005
            ],
            "kind": "Player"
          },
          "200670005": {
            "type": "Text",
            "id": 200670005,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "Honestly, I never considered not working as a pilot",
            "option": null,
            "nextIds": [
              200670006
            ],
            "kind": "NPC"
          },
          "200670006": {
            "type": "Text",
            "id": 200670006,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "If my dad saw me now, he might yell at me for being a coward who ran away from the battlefield",
            "option": null,
            "nextIds": [
              200670007,
              200670008
            ],
            "kind": "NPC"
          },
          "200670007": {
            "type": "Text",
            "id": 200670007,
            "sectionId": 2006700,
            "senderId": null,
            "text": "A desk job is also a battlefield, it mustn't have been easy to reach this point",
            "option": "The job market is a battlefield",
            "nextIds": [
              200670009
            ],
            "kind": "Player"
          },
          "200670009": {
            "type": "Text",
            "id": 200670009,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "Yeah... I'm different from dad...",
            "option": null,
            "nextIds": [
              200670010
            ],
            "kind": "NPC"
          },
          "200670010": {
            "type": "Text",
            "id": 200670010,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "I just have one wish for my child...",
            "option": null,
            "nextIds": [
              200670011
            ],
            "kind": "NPC"
          },
          "200670011": {
            "type": "Text",
            "id": 200670011,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "It doesn't matter if she's unexceptional, I just hope she's safe and sound",
            "option": null,
            "nextIds": [
              200670012,
              200670015
            ],
            "kind": "NPC"
          },
          "200670012": {
            "type": "Text",
            "id": 200670012,
            "sectionId": 2006700,
            "senderId": null,
            "text": "I don't have a child, I wouldn't know...",
            "option": "I have no kids. Can't relate.",
            "nextIds": [
              200670013
            ],
            "kind": "Player"
          },
          "200670013": {
            "type": "Text",
            "id": 200670013,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "That's right, maybe talking to you about this is a little too soon",
            "option": null,
            "nextIds": [
              200670014
            ],
            "kind": "NPC"
          },
          "200670014": {
            "type": "Text",
            "id": 200670014,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "You're still young, after all",
            "option": null,
            "nextIds": [
              200670017
            ],
            "kind": "NPC"
          },
          "200670017": {
            "type": "Text",
            "id": 200670017,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "Some things can only be understood through experience just like I did",
            "option": null,
            "nextIds": [
              200670018
            ],
            "kind": "NPC"
          },
          "200670018": {
            "type": "Text",
            "id": 200670018,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "So many years have passed...",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "200670015": {
            "type": "Text",
            "id": 200670015,
            "sectionId": 2006700,
            "senderId": null,
            "text": "I can understand a parent's wishes, but your children should have a say too",
            "option": "Your children should have a say",
            "nextIds": [
              200670016
            ],
            "kind": "Player"
          },
          "200670016": {
            "type": "Text",
            "id": 200670016,
            "sectionId": 2006700,
            "senderId": 1207,
            "text": "They're still young, after all",
            "option": null,
            "nextIds": [
              200670017
            ],
            "kind": "NPC"
          },
          "200670008": {
            "type": "Text",
            "id": 200670008,
            "sectionId": 2006700,
            "senderId": null,
            "text": "Your life, your choice. As long as it fulfills you",
            "option": "It's your choice to make",
            "nextIds": [
              200670009
            ],
            "kind": "Player"
          },
          "200670003": {
            "type": "Text",
            "id": 200670003,
            "sectionId": 2006700,
            "senderId": null,
            "text": "Not quite what I thought it would be, but it's not bad",
            "option": "Not the same",
            "nextIds": [
              200670005
            ],
            "kind": "Player"
          },
          "200670004": {
            "type": "Text",
            "id": 200670004,
            "sectionId": 2006700,
            "senderId": null,
            "text": "I don't know, I can't remember it that well",
            "option": "Can't remember",
            "nextIds": [
              200670005
            ],
            "kind": "Player"
          }
        }
      }
    ],
