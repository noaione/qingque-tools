          },
          "200680020": {
            "type": "Text",
            "id": 200680020,
            "sectionId": 2006800,
            "senderId": null,
            "text": "Something the matter?",
            "option": "Something the matter?",
            "nextIds": [
              200680021
            ],
            "kind": "Player"
          },
          "200680021": {
            "type": "Text",
            "id": 200680021,
            "sectionId": 2006800,
            "senderId": 1207,
            "text": "A cycrane delivered an official document, and an emergency meeting is coming up, I gotta go back to the commission",
            "option": null,
            "nextIds": [
              200680022,
              200680026
            ],
            "kind": "NPC"
          },
          "200680022": {
