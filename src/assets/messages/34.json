          "120050002": {
            "type": "Text",
            "id": 120050002,
            "sectionId": 1200500,
            "senderId": 34,
            "text": "Your friend request has been approved. You may now start chatting.",
            "option": null,
            "nextIds": [
              120050003
            ],
            "kind": "NPC"
          },
          "120050003": {
            "type": "Text",
            "id": 120050003,
            "sectionId": 1200500,
            "senderId": 34,
            "text": "Welcome, lonesome traveler.",
            "option": null,
            "nextIds": [
              120050004
            ],
            "kind": "NPC"
          },
          "120050004": {
            "type": "Text",
            "id": 120050004,
            "sectionId": 1200500,
            "senderId": 34,
            "text": "I've heard what happened. Our meeting in this sea of strangers could be a type of fated encounter.",
            "option": null,
            "nextIds": [
              120050005
            ],
            "kind": "NPC"
          },
          "120050005": {
            "type": "Text",
            "id": 120050005,
            "sectionId": 1200500,
            "senderId": 34,
            "text": "From today on, I am your friend in which you can confide your secrets. I will not break confidence and reveal anything you share with another person. I hope you will do the same.",
            "option": null,
            "nextIds": [
              120050006,
              120050008
            ],
            "kind": "NPC"
          },
          "120050006": {
            "type": "Text",
            "id": 120050006,
            "sectionId": 1200500,
            "senderId": null,
            "text": "Really? You're not some kind of high-quality imitation of the real one?",
            "option": "I don't believe you are who you say you are",
            "nextIds": [
              120050007
            ],
            "kind": "Player"
          },
          "120050007": {
            "type": "Text",
            "id": 120050007,
            "sectionId": 1200500,
            "senderId": 34,
            "text": "Am I that unworthy of trust? That is a little disappointing, but believe what you will.",
            "option": null,
            "nextIds": [
              120050010
            ],
            "kind": "NPC"
          },
          "120050010": {
            "type": "Text",
            "id": 120050010,
            "sectionId": 1200500,
            "senderId": 34,
            "text": "This picture is for you, as my meeting gift to you. I hope our communication will soon be more in-depth.",
            "option": null,
            "nextIds": [
              120050011
            ],
            "kind": "NPC"
          },
          "120050011": {
            "type": "Image",
            "id": 120050011,
            "sectionId": 1200500,
            "senderId": 34,
            "text": "[Image]",
            "option": null,
            "nextIds": [],
            "kind": "NPC",
            "image": {
              "id": 10202,
              "path": "image/messages/E10006.png"
            }
          },
          "120050008": {
            "type": "Text",
            "id": 120050008,
            "sectionId": 1200500,
            "senderId": null,
            "text": "Don't worry, miss. I will not expose you",
            "option": "I won't expose you",
            "nextIds": [
              120050009
            ],
            "kind": "Player"
          },
          "120050009": {
            "type": "Text",
            "id": 120050009,
            "sectionId": 1200500,
            "senderId": 34,
            "text": "Good, let that be a promise between us.",
            "option": null,
            "nextIds": [
              120050010
            ],
            "kind": "NPC"
          }
        }
      },
      {
        "type": "Message",
