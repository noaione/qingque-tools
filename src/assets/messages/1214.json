          },
          "201170003": {
            "type": "Text",
            "id": 201170003,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "|",
            "option": null,
            "nextIds": [
              201170004,
              201170006,
              201170008
            ],
            "kind": "NPC"
          },
          "201170004": {
            "type": "Text",
            "id": 201170004,
            "sectionId": 2011700,
            "senderId": null,
            "text": "Madam Xueyi, what happened to you? What's going on?",
            "option": "Madam Xueyi, what happened to you?",
            "nextIds": [
              201170005
            ],
            "kind": "Player"
          },
          "201170005": {
            "type": "Text",
            "id": 201170005,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "Nothing.",
            "option": null,
            "nextIds": [
              201170010
            ],
            "kind": "NPC"
          },
          "201170010": {
            "type": "Text",
            "id": 201170010,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "I'm just trying out the emoticons",
            "option": null,
            "nextIds": [
              201170011
            ],
            "kind": "NPC"
          },
          "201170011": {
            "type": "Text",
            "id": 201170011,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "Huohuo says I'm often expressionless",
            "option": null,
            "nextIds": [
              201170012
            ],
            "kind": "NPC"
          },
          "201170012": {
            "type": "Text",
            "id": 201170012,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "And even my text messages sound too serious",
            "option": null,
            "nextIds": [
              201170013
            ],
            "kind": "NPC"
          },
          "201170013": {
            "type": "Text",
            "id": 201170013,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "Per her suggestion, I am now testing these emoticons",
            "option": null,
            "nextIds": [
              201170014
            ],
            "kind": "NPC"
          },
          "201170014": {
            "type": "Text",
            "id": 201170014,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "(´_っ`)",
            "option": null,
            "nextIds": [
              201170015
            ],
            "kind": "NPC"
          },
          "201170015": {
            "type": "Text",
            "id": 201170015,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "What do you think?",
            "option": null,
            "nextIds": [
              201170016,
              201170020,
              201170024
            ],
            "kind": "NPC"
          },
          "201170016": {
            "type": "Text",
            "id": 201170016,
            "sectionId": 2011700,
            "senderId": null,
            "text": "Truth be told, I'm not quite used to it. This isn't the Madam Xueyi I'm familiar with.",
            "option": "I'm not quite used to it",
            "nextIds": [
              201170017
            ],
            "kind": "Player"
          },
          "201170017": {
            "type": "Text",
            "id": 201170017,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "Hmm",
            "option": null,
            "nextIds": [
              201170018
            ],
            "kind": "NPC"
          },
          "201170018": {
            "type": "Text",
            "id": 201170018,
            "sectionId": 2011700,
            "senderId": 1214,
            "text": "I don't think there's much difference.",
            "option": null,
            "nextIds": [
              201170019
            ],
            "kind": "NPC"
