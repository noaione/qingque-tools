        "messages": {
          "130680000": {
            "type": "Text",
            "id": 130680000,
            "sectionId": 1306800,
            "senderId": 1112,
            "text": "The IPC's Special Investigations Team is wrapping up here",
            "option": null,
            "nextIds": [
              130680001
            ],
            "kind": "NPC"
          },
          "130680001": {
            "type": "Text",
            "id": 130680001,
            "sectionId": 1306800,
            "senderId": 1112,
            "text": "We found a total of <unbreak>329</unbreak> dolls, but no bomb",
            "option": null,
            "nextIds": [
              130680002
            ],
            "kind": "NPC"
          },
          "130680002": {
            "type": "Text",
            "id": 130680002,
            "sectionId": 1306800,
            "senderId": 1112,
            "text": "Thought I'd drop this update with you guys",
            "option": null,
            "nextIds": [
              130680003
            ],
            "kind": "NPC"
          },
          "130680003": {
            "type": "Text",
            "id": 130680003,
            "sectionId": 1306800,
            "senderId": 1315,
            "text": "Even you guys can't do anything about it?",
            "option": null,
            "nextIds": [
              130680004
            ],
            "kind": "NPC"
          },
          "130680004": {
            "type": "Text",
            "id": 130680004,
            "sectionId": 1306800,
            "senderId": 1315,
            "text": "Holy forkin' shirtballs, this is going to be a killer",
            "option": null,
            "nextIds": [
              130680005
            ],
            "kind": "NPC"
          },
          "130680005": {
            "type": "Text",
            "id": 130680005,
            "sectionId": 1306800,
            "senderId": 1001,
            "text": "Don't give up! We're so close to <unbreak>999</unbreak>",
            "option": null,
            "nextIds": [
              130680006
            ],
            "kind": "NPC"
          },
          "130680006": {
            "type": "Text",
            "id": 130680006,
            "sectionId": 1306800,
            "senderId": 1001,
            "text": "The Express Crew is also about to be done!",
            "option": null,
            "nextIds": [
              130680007
            ],
            "kind": "NPC"
          },
          "130680007": {
            "type": "Text",
            "id": 130680007,
            "sectionId": 1306800,
            "senderId": 1112,
            "text": "Keep up the good work, everyone. I'll be taking over the evac team now",
            "option": null,
            "nextIds": [
              130680008
            ],
            "kind": "NPC"
          },
          "130680008": {
            "type": "Text",
            "id": 130680008,
            "sectionId": 1306800,
            "senderId": 1112,
            "text": "Good luck, everyone",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          }
        }
      }
