          "200440005": {
            "type": "Text",
            "id": 200440005,
            "sectionId": 2004400,
            "senderId": 1103,
            "text": "I screwed up",
            "option": null,
            "nextIds": [
              200440006
            ],
            "kind": "NPC"
          },
          "200440006": {
            "type": "Text",
            "id": 200440006,
            "sectionId": 2004400,
            "senderId": 1103,
            "text": "A while ago, a thought occurred to me and I installed an automaton chainsaw on a vehicle",
