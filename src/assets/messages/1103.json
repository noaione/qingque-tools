            "nextIds": [
              200440025
            ],
            "kind": "NPC"
          },
          "200440025": {
            "type": "Text",
            "id": 200440025,
            "sectionId": 2004400,
            "senderId": 1103,
            "text": "Shoot, someone's coming",
