        "messages": {
          "111050000": {
            "type": "Text",
            "id": 111050000,
            "sectionId": 1110500,
            "senderId": 1103,
            "text": "Hey, {NICKNAME}! I just want to thank you for helping Molly out",
            "option": null,
            "nextIds": [
              111050001,
              111050002,
              111050003
            ],
            "kind": "NPC"
          },
          "111050001": {
            "type": "Text",
            "id": 111050001,
            "sectionId": 1110500,
            "senderId": null,
            "text": "It's the least I could do",
            "option": "It's the least I could do",
            "nextIds": [
              111050004
            ],
            "kind": "Player"
          },
          "111050004": {
            "type": "Text",
            "id": 111050004,
            "sectionId": 1110500,
            "senderId": 1103,
            "text": "Haha, well you just like helping everyone, don't you?",
            "option": null,
            "nextIds": [
              111050007
            ],
            "kind": "NPC"
          },
          "111050007": {
            "type": "Text",
            "id": 111050007,
            "sectionId": 1110500,
            "senderId": 1103,
            "text": "Come down to the workshop if you can. Molly is not as grumpy as she seems. She actually wants company. And she'd be so happy if you could have a chat with her. That's all I wanted to say. See ya!",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "111050002": {
            "type": "Text",
            "id": 111050002,
            "sectionId": 1110500,
            "senderId": null,
            "text": "Pela did great work",
            "option": "Pela did great work",
            "nextIds": [
              111050005
            ],
            "kind": "Player"
          },
          "111050005": {
            "type": "Text",
            "id": 111050005,
            "sectionId": 1110500,
            "senderId": 1103,
            "text": "Take the compliment, will you? Pela told me everything",
            "option": null,
            "nextIds": [
              111050007
            ],
            "kind": "NPC"
          },
          "111050003": {
            "type": "Text",
            "id": 111050003,
            "sectionId": 1110500,
            "senderId": null,
            "text": "Is she okay?",
            "option": "Is she okay?",
            "nextIds": [
              111050006
            ],
            "kind": "Player"
          },
          "111050006": {
            "type": "Text",
            "id": 111050006,
            "sectionId": 1110500,
            "senderId": 1103,
            "text": "She's fine and in pretty good shape. Don't worry, I'll look after her",
            "option": null,
            "nextIds": [
              111050007
            ],
            "kind": "NPC"
          }
        },
        "mission": {
          "id": 2011301,
          "name": "Ancestor's Drill Bit",
          "type": "Branch"
        }
      }
