          },
          "200440017": {
            "type": "Text",
            "id": 200440017,
            "sectionId": 2004400,
            "senderId": null,
            "text": "No problem, I will be as honest as possible",
            "option": "I'll answer honestly",
            "nextIds": [
              200440018
            ],
            "kind": "Player"
          },
          "200440018": {
            "type": "Text",
            "id": 200440018,
            "sectionId": 2004400,
            "senderId": 1103,
            "text": "!!!",
            "option": null,
            "nextIds": [
              200440019
            ],
            "kind": "NPC"
          },
          "200440019": {
            "type": "Text",
            "id": 200440019,
            "sectionId": 2004400,
            "senderId": 1103,
            "text": "Even you have become so untrustworthy",
            "option": null,
            "nextIds": [
              200440025
            ],
            "kind": "NPC"
          },
          "200440021": {
            "type": "Text",
            "id": 200440021,
            "sectionId": 2004400,
