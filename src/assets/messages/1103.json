        "messages": {
          "111150000": {
            "type": "Text",
            "id": 111150000,
            "sectionId": 1111500,
            "senderId": 1103,
            "text": "{NICKNAME}, are you there?",
            "option": null,
            "nextIds": [
              111150001
            ],
            "kind": "NPC"
          },
          "111150001": {
            "type": "Text",
            "id": 111150001,
            "sectionId": 1111500,
            "senderId": 1103,
            "text": "When I was cleaning, I found two coupons for afternoon tea at the Goethe Hotel. Do you want to come along?",
            "option": null,
            "nextIds": [
              111150002,
              111150003,
              111150004
            ],
            "kind": "NPC"
          },
          "111150002": {
            "type": "Text",
            "id": 111150002,
            "sectionId": 1111500,
            "senderId": null,
            "text": "You can actually clean? Are you for real?",
            "option": "You can actually clean?",
            "nextIds": [
              111150005
            ],
            "kind": "Player"
          },
          "111150005": {
            "type": "Text",
            "id": 111150005,
            "sectionId": 1111500,
            "senderId": 1103,
            "text": "Haha, I knew you would say this!",
            "option": null,
            "nextIds": [
              111150008
            ],
            "kind": "NPC"
          },
          "111150008": {
            "type": "Text",
            "id": 111150008,
            "sectionId": 1111500,
            "senderId": 1103,
            "text": "Let's meet beside the lobby. I'll be waiting!",
            "option": null,
            "nextIds": [
              111150009,
              111150010,
              111150011
            ],
            "kind": "NPC"
          },
          "111150009": {
            "type": "Text",
            "id": 111150009,
            "sectionId": 1111500,
            "senderId": null,
            "text": "Wait, when are we meeting?",
            "option": "Wait, when are we meeting?",
            "nextIds": [
              111150012,
              111150013,
              111150014
            ],
            "kind": "Player"
          },
          "111150012": {
            "type": "Text",
            "id": 111150012,
            "sectionId": 1111500,
            "senderId": null,
            "text": "???",
            "option": "???",
            "nextIds": [],
            "kind": "Player"
          },
          "111150013": {
            "type": "Text",
            "id": 111150013,
            "sectionId": 1111500,
            "senderId": null,
            "text": "Hello, you there?",
            "option": "Hello, you there?",
            "nextIds": [],
            "kind": "Player"
          },
          "111150014": {
            "type": "Text",
            "id": 111150014,
            "sectionId": 1111500,
            "senderId": null,
            "text": "Is it sent?",
            "option": "Is it sent? Can you see the text?",
            "nextIds": [
              111150015
            ],
            "kind": "Player"
          },
          "111150015": {
            "type": "Text",
            "id": 111150015,
            "sectionId": 1111500,
            "senderId": null,
            "text": "Hello?",
            "option": null,
            "nextIds": [],
            "kind": "PlayerAuto"
          },
          "111150010": {
            "type": "Text",
            "id": 111150010,
            "sectionId": 1111500,
            "senderId": null,
            "text": "There are so many tables beside the lobby. Which one?",
            "option": "Which table beside the lobby?",
            "nextIds": [
              111150012,
              111150013,
              111150014
            ],
            "kind": "Player"
          },
          "111150011": {
            "type": "Text",
            "id": 111150011,
            "sectionId": 1111500,
            "senderId": null,
            "text": "I'll be there immediately. Order something for me.",
            "option": "Order a drink for me...",
            "nextIds": [
              111150012,
              111150013,
              111150014
            ],
            "kind": "Player"
          },
          "111150003": {
            "type": "Text",
            "id": 111150003,
            "sectionId": 1111500,
            "senderId": null,
            "text": "What made you think of me? Where's Pela?",
            "option": "Why not Pela...",
            "nextIds": [
              111150006
            ],
            "kind": "Player"
          },
          "111150006": {
            "type": "Text",
            "id": 111150006,
            "sectionId": 1111500,
            "senderId": 1103,
            "text": "She's either writing reports, rushing scripts, volunteering, or sleeping. There's never a good time to ask her out",
            "option": null,
            "nextIds": [
              111150008
            ],
            "kind": "NPC"
          },
          "111150004": {
            "type": "Text",
            "id": 111150004,
            "sectionId": 1111500,
            "senderId": null,
            "text": "Why would I let it go to waste if it's for free? Just wait for me.",
            "option": "Free drinks? Count me in",
            "nextIds": [
              111150007
            ],
            "kind": "Player"
          },
          "111150007": {
            "type": "Text",
            "id": 111150007,
            "sectionId": 1111500,
            "senderId": 1103,
            "text": "It's a date then! Whoever stands the other up is a snow mouse",
            "option": null,
            "nextIds": [
              111150008
            ],
            "kind": "NPC"
          }
        },
        "mission": {
          "id": 2011402,
          "name": "Time Express",
          "type": "Companion"
        }
      }
