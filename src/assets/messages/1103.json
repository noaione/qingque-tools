            "nextIds": [
              200440012,
              200440017,
              200440021
            ],
            "kind": "NPC"
          },
          "200440012": {
            "type": "Text",
            "id": 200440012,
            "sectionId": 2004400,
            "senderId": null,
            "text": "No worries, your secret is safe with me",
