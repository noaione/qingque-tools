            "nextIds": [
              200220003
            ],
            "kind": "NPC"
          },
          "200220003": {
            "type": "Text",
            "id": 200220003,
            "sectionId": 2002200,
            "senderId": 1103,
            "text": "Can you contribute a comment or two, {NICKNAME}?",
            "option": null,
            "nextIds": [
              200220004
            ],
            "kind": "NPC"
          },
          "200220004": {
            "type": "Text",
            "id": 200220004,
            "sectionId": 2002200,
            "senderId": null,
            "text": "I'll think about it",
            "option": "I'll think about it",
            "nextIds": [
              200220029
            ],
            "kind": "Player"
          },
          "200220029": {
            "type": "Text",
            "id": 200220029,
            "sectionId": 2002200,
            "senderId": null,
            "text": "OK",
            "option": null,
            "nextIds": [
              200220030
            ],
            "kind": "PlayerAuto"
          },
          "200220030": {
            "type": "Text",
            "id": 200220030,
            "sectionId": 2002200,
            "senderId": null,
            "text": "I've got it",
            "option": null,
            "nextIds": [
              200220005,
              200220010,
              200220018
