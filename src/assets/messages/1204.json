            "sticker": {
              "id": 20008,
              "path": "icon/emoji/20008.png",
              "keywords": "Shocked"
            }
          },
          "201020012": {
            "type": "Text",
            "id": 201020012,
            "sectionId": 2010200,
            "senderId": null,
            "text": "I only told the conductor and Dan Heng, no one else. Pom-Pom seems a little worried...",
            "option": null,
            "nextIds": [
              201020013
            ],
            "kind": "PlayerAuto"
          },
          "201020013": {
            "type": "Text",
            "id": 201020013,
            "sectionId": 2010200,
            "senderId": 1204,
            "text": "Please relay my apology to the conductor for the rudeness of my mentor.",
            "option": null,
            "nextIds": [
              201020015
            ],
            "kind": "NPC"
          },
          "201020014": {
            "type": "Text",
            "id": 201020014,
            "sectionId": 2010200,
            "senderId": null,
            "text": "Dan Heng looked really serious after I received the letter. Is he okay?",
            "option": "Is Dan Heng okay?",
            "nextIds": [
              201020015
            ],
            "kind": "Player"
          },
          "201020004": {
            "type": "Text",
            "id": 201020004,
            "sectionId": 2010200,
            "senderId": null,
            "text": "What made you think of us, General?",
            "option": "What made you think of us?",
            "nextIds": [
              201020005
            ],
            "kind": "Player"
          },
          "201020005": {
            "type": "Text",
            "id": 201020005,
            "sectionId": 2010200,
            "senderId": 1204,
            "text": "The Express Crew are my friends who went through life and death with me. Isn't it normal to think about my friends?",
            "option": null,
