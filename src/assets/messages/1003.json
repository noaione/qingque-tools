        ],
        "messages": {
          "200070000": {
            "type": "Text",
            "id": 200070000,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "Let's make sure we get back to the Express in time for dinner.",
            "option": null,
            "nextIds": [
              200070002
            ],
            "kind": "NPC"
          },
          "200070002": {
            "type": "Text",
            "id": 200070002,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "We just got a new shipment of fresh supplies. Anything strike your fancy?",
            "option": null,
            "nextIds": [
              200070003,
              200070004,
              200070005
            ],
            "kind": "NPC"
          },
          "200070003": {
            "type": "Text",
            "id": 200070003,
            "sectionId": 2000700,
            "senderId": null,
            "text": "Not a picky eater, anything edible strikes my fancy.",
            "option": "Not a picky eater, anything edible strikes my fancy.",
            "nextIds": [
              200070006
            ],
            "kind": "Player"
          },
          "200070006": {
            "type": "Sticker",
            "id": 200070006,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "[Confused]",
            "option": null,
            "nextIds": [
              200070007
            ],
            "kind": "NPC",
            "sticker": {
              "id": 20007,
              "path": "icon/emoji/20007.png",
              "keywords": "Puzzled"
            }
          },
          "200070007": {
            "type": "Text",
            "id": 200070007,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "You say you're not picky, and Dan Heng says he's good with whatever... Guess I'll just have to cook whatever I feel like.",
            "option": null,
            "nextIds": [
              200070008
            ],
            "kind": "NPC"
          },
          "200070008": {
            "type": "Text",
            "id": 200070008,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "Today's meal orders:\\nPom-Pom: Crispy Crumbly Pom-Pom Pie\\nMarch 7th: Tomato fried egg and rice, without the tomato\\nDan Heng: Whatever\\nWelt: Assorted salt-roasted fish with onion rings\\n{NICKNAME}: Anything edible\\nHimeko: The \"whatever I feel like\" special",
            "option": null,
            "nextIds": [
              200070015
            ],
            "kind": "NPC"
          },
          "200070015": {
            "type": "Sticker",
            "id": 200070015,
            "sectionId": 2000700,
            "senderId": null,
            "text": "[Thanks]",
            "option": null,
            "nextIds": [],
            "kind": "Player",
            "sticker": {
              "id": 20002,
              "path": "icon/emoji/20002.png",
              "keywords": "Finger Heart"
            }
          },
          "200070004": {
            "type": "Text",
            "id": 200070004,
            "sectionId": 2000700,
            "senderId": null,
            "text": "Meat! Load me up on meat!",
            "option": "Meat! Load me up on meat!",
            "nextIds": [
              200070009
            ],
            "kind": "Player"
          },
          "200070009": {
            "type": "Sticker",
            "id": 200070009,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "[Okay]",
            "option": null,
            "nextIds": [
              200070010
            ],
            "kind": "NPC",
            "sticker": {
              "id": 20004,
              "path": "icon/emoji/20004.png",
              "keywords": "Understood"
            }
          },
          "200070010": {
            "type": "Text",
            "id": 200070010,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "You've had a long day, better let your body refuel!",
            "option": null,
            "nextIds": [
              200070011
            ],
            "kind": "NPC"
          },
          "200070011": {
            "type": "Text",
            "id": 200070011,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "Today's meal orders:\\nPom-Pom: Crispy Crumbly Pom-Pom Pie\\nMarch 7th: Tomato fried egg and rice, without the tomato\\nDan Heng: Whatever\\nWelt: Assorted salt-roasted fish with onion rings\\n{NICKNAME}: Loads of meat\\nHimeko: The Express Special Supreme",
            "option": null,
            "nextIds": [
              200070015
            ],
            "kind": "NPC"
          },
          "200070005": {
            "type": "Text",
            "id": 200070005,
            "sectionId": 2000700,
            "senderId": null,
            "text": "I can't imagine a life without milk tea.",
            "option": "I can't imagine a life without milk tea.",
            "nextIds": [
              200070012
            ],
            "kind": "Player"
          },
          "200070012": {
            "type": "Sticker",
            "id": 200070012,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "[Shocked]",
            "option": null,
            "nextIds": [
              200070013
            ],
            "kind": "NPC",
            "sticker": {
              "id": 20008,
              "path": "icon/emoji/20008.png",
              "keywords": "Shocked"
            }
          },
          "200070013": {
            "type": "Text",
            "id": 200070013,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "But milk tea doesn't count as a meal... Well, I'll whip something up.",
            "option": null,
            "nextIds": [
              200070014
            ],
            "kind": "NPC"
          },
          "200070014": {
            "type": "Text",
            "id": 200070014,
            "sectionId": 2000700,
            "senderId": 1003,
            "text": "Today's meal orders:\\nPom-Pom: Crispy Crumbly Pom-Pom Pie\\nMarch 7th: Tomato fried egg and rice, without the tomato\\nDan Heng: Whatever\\nWelt: Assorted salt-roasted fish with onion rings\\n{NICKNAME}: Milk tea\\nHimeko: The Express Special with milk tea soup base",
            "option": null,
            "nextIds": [
              200070015
            ],
            "kind": "NPC"
          }
        }
      }
