          },
          "115000001": {
            "type": "Text",
            "id": 115000001,
            "sectionId": 1150000,
            "senderId": 1000,
            "text": "...Don't ignore my message! I've got some good rewards for you!",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          }
        }
      }
