        "messages": {
          "101230101": {
            "type": "Text",
            "id": 101230101,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "Aaaaaahh!",
            "option": null,
            "nextIds": [
              101230102
            ],
            "kind": "NPC"
          },
          "101230102": {
            "type": "Text",
            "id": 101230102,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "{NICKNAME}! Are you there!?",
            "option": null,
            "nextIds": [
              101230103,
              101230104
            ],
            "kind": "NPC"
          },
          "101230103": {
            "type": "Text",
            "id": 101230103,
            "sectionId": 1012301,
            "senderId": null,
            "text": "What's going on?",
            "option": "What's the hurry?",
            "nextIds": [
              101230105
            ],
            "kind": "Player"
          },
          "101230105": {
            "type": "Text",
            "id": 101230105,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "It's something super urgent!",
            "option": null,
            "nextIds": [
              101230106
            ],
            "kind": "NPC"
          },
          "101230106": {
            "type": "Text",
            "id": 101230106,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "Pom-Pom doesn't even want to think about it...",
            "option": null,
            "nextIds": [
              101230107
            ],
            "kind": "NPC"
          },
          "101230107": {
            "type": "Text",
            "id": 101230107,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "The train's long overdue cleaning day is almost upon us! Aaaaahhh!",
            "option": null,
            "nextIds": [
              101230108
            ],
            "kind": "NPC"
          },
          "101230108": {
            "type": "Text",
            "id": 101230108,
            "sectionId": 1012301,
            "senderId": null,
            "text": "...Huh?",
            "option": "...",
            "nextIds": [
              101230109
            ],
            "kind": "Player"
          },
          "101230109": {
            "type": "Text",
            "id": 101230109,
            "sectionId": 1012301,
            "senderId": null,
            "text": "I get it now.",
            "option": null,
            "nextIds": [
              101230110
            ],
            "kind": "PlayerAuto"
          },
          "101230110": {
            "type": "Text",
            "id": 101230110,
            "sectionId": 1012301,
            "senderId": null,
            "text": "So, you want me to come back and help you with the cleaning?",
            "option": null,
            "nextIds": [
              101230111
            ],
            "kind": "PlayerAuto"
          },
          "101230111": {
            "type": "Sticker",
            "id": 101230111,
            "sectionId": 1012301,
            "senderId": null,
            "text": "[cry]",
            "option": null,
            "nextIds": [
              101230112
            ],
            "kind": "PlayerAuto",
            "sticker": {
              "id": 20005,
              "path": "icon/emoji/20005.png",
              "keywords": "Crying"
            }
          },
          "101230112": {
            "type": "Text",
            "id": 101230112,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "Not really...",
            "option": null,
            "nextIds": [
              101230113
            ],
            "kind": "NPC"
          },
          "101230113": {
            "type": "Text",
            "id": 101230113,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "But if you're willing to lend a hand, {NICKNAME}, that would be great!",
            "option": null,
            "nextIds": [
              101230114
            ],
            "kind": "NPC"
          },
          "101230114": {
            "type": "Text",
            "id": 101230114,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "We're on our way to the nearest Washtopia.",
            "option": null,
            "nextIds": [
              101230115
            ],
            "kind": "NPC"
          },
          "101230115": {
            "type": "Text",
            "id": 101230115,
            "sectionId": 1012301,
            "senderId": 1000,
            "text": "Come back and see me when you have time!",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "101230104": {
            "type": "Text",
            "id": 101230104,
            "sectionId": 1012301,
            "senderId": null,
            "text": "Take your time, okay?",
            "option": "Calm down.",
            "nextIds": [
              101230105
            ],
            "kind": "Player"
          }
        },
        "mission": {
          "id": 2000801,
          "name": "Night of Universal Hallucinations",
          "type": "Companion"
        }
      }
