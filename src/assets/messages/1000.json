            "senderId": 1000,
            "text": "I feel you haven't been back to the Express since forever, {NICKNAME}...",
            "option": null,
            "nextIds": [
              500090001,
              500090003
            ],
            "kind": "NPC"
          },
          "500090001": {
            "type": "Text",
            "id": 500090001,
            "sectionId": 5000900,
            "senderId": null,
            "text": "It hasn't been that long since we last said goodbye, right?",
            "option": "Didn't we just see each other?",
            "nextIds": [
              500090002
            ],
            "kind": "Player"
          },
          "500090002": {
            "type": "Text",
            "id": 500090002,
            "sectionId": 5000900,
            "senderId": 1000,
            "text": "Don't worry about the details!",
            "option": null,
            "nextIds": [
              500090005
            ],
            "kind": "NPC"
          },
          "500090005": {
            "type": "Text",
            "id": 500090005,
            "sectionId": 5000900,
            "senderId": 1000,
            "text": "Come back when you've got the time. Pom-Pom has some things for you",
            "option": null,
            "nextIds": [
              500090006
            ],
            "kind": "NPC"
          },
          "500090006": {
            "type": "Sticker",
            "id": 500090006,
            "sectionId": 5000900,
            "senderId": 1000,
            "text": "[Thumbs Up]",
            "option": null,
            "nextIds": [],
            "kind": "NPC",
            "sticker": {
              "id": 20006,
              "path": "icon/emoji/20006.png",
              "keywords": "Like"
            }
          },
          "500090003": {
            "type": "Text",
            "id": 500090003,
            "sectionId": 5000900,
            "senderId": null,
            "text": "Mmm... It has been a while. I'm starting to forget what Pom-Pom looks like",
            "option": "It has been a while...",
            "nextIds": [
              500090004
            ],
            "kind": "Player"
          },
          "500090004": {
            "type": "Text",
            "id": 500090004,
            "sectionId": 5000900,
            "senderId": 1000,
            "text": "It hasn't been THAT long!",
            "option": null,
            "nextIds": [
              500090005
            ],
            "kind": "NPC"
          }
        },
        "mission": {
          "id": 4050005,
          "name": "The Conductor's Rewards",
          "type": "Branch"
        }
