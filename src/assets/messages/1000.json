          "500110006": {
            "type": "Text",
            "id": 500110006,
            "sectionId": 5001100,
            "senderId": 1000,
            "text": "Ahh, typo typo! It's \"you've been,\" not \"your belly\"!",
            "option": null,
            "nextIds": [
              500110007
            ],
            "kind": "NPC"
          },
          "500110007": {
            "type": "Text",
            "id": 500110007,
            "sectionId": 5001100,
            "senderId": 1000,
