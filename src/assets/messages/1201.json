    {
      "type": "Message",
      "id": 2005800,
      "startIds": [
        200580000
      ],
      "messages": {
        "200580000": {
          "type": "Text",
          "id": 200580000,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Hey have you registered for the new immersia that just came out",
          "option": null,
          "nextIds": [
            200580001
          ],
          "kind": "NPC"
        },
        "200580001": {
          "type": "Text",
          "id": 200580001,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Come be my buddy",
          "option": null,
          "nextIds": [
            200580002,
            200580005
          ],
          "kind": "NPC"
        },
        "200580002": {
          "type": "Text",
          "id": 200580002,
          "sectionId": 2005800,
          "senderId": null,
          "text": "Buddy?",
          "option": "Buddy?",
          "nextIds": [
            200580003
          ],
          "kind": "Player"
        },
        "200580003": {
          "type": "Text",
          "id": 200580003,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Oh, I call those who play games of Celestial Jade with us buddies",
          "option": null,
          "nextIds": [
            200580004
          ],
          "kind": "NPC"
        },
        "200580004": {
          "type": "Text",
          "id": 200580004,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "But I was talking about the new Foxian immersia called Viridian Soul, we call those buddies too",
          "option": null,
          "nextIds": [
            200580008
          ],
          "kind": "NPC"
        },
        "200580008": {
          "type": "Text",
          "id": 200580008,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Long as you have a jade abacus you can join in too, it's very easy",
          "option": null,
          "nextIds": [
            200580009
          ],
          "kind": "NPC"
        },
        "200580009": {
          "type": "Text",
          "id": 200580009,
          "sectionId": 2005800,
          "senderId": null,
          "text": "Ah got it, it's basically a mobile game",
          "option": "Basically a mobile game",
          "nextIds": [
            200580010
          ],
          "kind": "Player"
        },
        "200580010": {
          "type": "Text",
          "id": 200580010,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "? What's a mobile game? I don't understand",
          "option": null,
          "nextIds": [
            200580011
          ],
          "kind": "NPC"
        },
        "200580011": {
          "type": "Text",
          "id": 200580011,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Anyway, I've already registered, it's free",
          "option": null,
          "nextIds": [
            200580012
          ],
          "kind": "NPC"
        },
        "200580012": {
          "type": "Text",
          "id": 200580012,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Now I can play it at work",
          "option": null,
          "nextIds": [
            200580013
          ],
          "kind": "NPC"
        },
        "200580013": {
          "type": "Text",
          "id": 200580013,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "You coming? I'll open up a friend room for us",
          "option": null,
          "nextIds": [
            200580015,
            200580020
          ],
          "kind": "NPC"
        },
        "200580015": {
          "type": "Text",
          "id": 200580015,
          "sectionId": 2005800,
          "senderId": null,
          "text": "I'd like to",
          "option": "I don't know how to play",
          "nextIds": [
            200580016
          ],
          "kind": "Player"
        },
        "200580016": {
          "type": "Text",
          "id": 200580016,
          "sectionId": 2005800,
          "senderId": null,
          "text": "But I don't know how to play Celestial Jade",
          "option": null,
          "nextIds": [
            200580014
          ],
          "kind": "PlayerAuto"
        },
        "200580014": {
          "type": "Sticker",
          "id": 200580014,
          "sectionId": 2005800,
          "senderId": null,
          "text": "[cry]",
          "option": "I don't know how to play",
          "nextIds": [
            200580017
          ],
          "kind": "PlayerAuto",
          "sticker": {
            "id": 20005,
            "path": "SpriteOutput/Emoji/20005.png",
            "keywords": "Crying"
          }
        },
        "200580017": {
          "type": "Text",
          "id": 200580017,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Oh, it's easy",
          "option": null,
          "nextIds": [
            200580018
          ],
          "kind": "NPC"
        },
        "200580018": {
          "type": "Text",
          "id": 200580018,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Come over to the games parlor when you have time, you'll pick it up in no time",
          "option": null,
          "nextIds": [
            200580023
          ],
          "kind": "NPC"
        },
        "200580023": {
          "type": "Text",
          "id": 200580023,
          "sectionId": 2005800,
          "senderId": null,
          "text": "Really?",
          "option": "Really?",
          "nextIds": [
            200580024
          ],
          "kind": "Player"
        },
        "200580024": {
          "type": "Text",
          "id": 200580024,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "I'm not lying",
          "option": null,
          "nextIds": [
            200580025
          ],
          "kind": "NPC"
        },
        "200580025": {
          "type": "Text",
          "id": 200580025,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "With the number of people on the Express, it should be easy to find enough players",
          "option": null,
          "nextIds": [
            200580026
          ],
          "kind": "NPC"
        },
        "200580026": {
          "type": "Text",
          "id": 200580026,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "You can teach everyone after learning it, then you'll be able to play together",
          "option": null,
          "nextIds": [
            200580027
          ],
          "kind": "NPC"
        },
        "200580027": {
          "type": "Text",
          "id": 200580027,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "I'll send you the invite in a bit",
          "option": null,
          "nextIds": [
            200580028
          ],
          "kind": "NPC"
        },
        "200580028": {
          "type": "Sticker",
          "id": 200580028,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "[Thumbs Up]",
          "option": null,
          "nextIds": [],
          "kind": "NPC",
          "sticker": {
            "id": 20006,
            "path": "SpriteOutput/Emoji/20006.png",
            "keywords": "Like"
          }
        },
        "200580020": {
          "type": "Text",
          "id": 200580020,
          "sectionId": 2005800,
          "senderId": null,
          "text": "Do you also play Celestial Jade when you're working?",
          "option": "Playing games while working?",
          "nextIds": [
            200580019
          ],
          "kind": "Player"
        },
        "200580019": {
          "type": "Sticker",
          "id": 200580019,
          "sectionId": 2005800,
          "senderId": null,
          "text": "[Confused]",
          "option": "Playing games while working?",
          "nextIds": [
            200580021
          ],
          "kind": "PlayerAuto",
          "sticker": {
            "id": 20007,
            "path": "SpriteOutput/Emoji/20007.png",
            "keywords": "Puzzled"
          }
        },
        "200580021": {
          "type": "Text",
          "id": 200580021,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Working is tiring, okay",
          "option": null,
          "nextIds": [
            200580022
          ],
          "kind": "NPC"
        },
        "200580022": {
          "type": "Text",
          "id": 200580022,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "It really releases stress, just try it",
          "option": null,
          "nextIds": [
            200580023
          ],
          "kind": "NPC"
        },
        "200580005": {
          "type": "Text",
          "id": 200580005,
          "sectionId": 2005800,
          "senderId": null,
          "text": "Immersia?",
          "option": "Immersia?",
          "nextIds": [
            200580006
          ],
          "kind": "Player"
        },
        "200580006": {
          "type": "Text",
          "id": 200580006,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Oh it's just a Foxian sympathetic immersia called Viridian Soul",
          "option": null,
          "nextIds": [
            200580007
          ],
          "kind": "NPC"
        },
        "200580007": {
          "type": "Text",
          "id": 200580007,
          "sectionId": 2005800,
          "senderId": 1201,
          "text": "Specifically for playing Celestial Jade, so we call them buddies",
          "option": null,
          "nextIds": [
            200580008
          ],
          "kind": "NPC"
        }
      }
    },
    {
      "type": "Message",
      "id": 2005900,
      "startIds": [
        200590000
      ],
      "messages": {
        "200590000": {
          "type": "Text",
          "id": 200590000,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "Oh right, I wanna ask for a favor",
          "option": null,
          "nextIds": [
            200590001
          ],
          "kind": "NPC"
        },
        "200590001": {
          "type": "Text",
          "id": 200590001,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "I went to listen to Mr. Xiyan's storytelling after work, but he stopped at a cliffhanger, I can't take it",
          "option": null,
          "nextIds": [
            200590002
          ],
          "kind": "NPC"
        },
        "200590002": {
          "type": "Text",
          "id": 200590002,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "He's resuming tomorrow, but at an earlier time",
          "option": null,
          "nextIds": [
            200590003
          ],
          "kind": "NPC"
        },
        "200590003": {
          "type": "Text",
          "id": 200590003,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "I don't know if I'll be able to get there soon enough after work",
          "option": null,
          "nextIds": [
            200590004
          ],
          "kind": "NPC"
        },
        "200590004": {
          "type": "Text",
          "id": 200590004,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "If I can't make it, can you go and listen in my stead?",
          "option": null,
          "nextIds": [
            200590006,
            200590009
          ],
          "kind": "NPC"
        },
        "200590006": {
          "type": "Text",
          "id": 200590006,
          "sectionId": 2005900,
          "senderId": null,
          "text": "Sure",
          "option": "No problem",
          "nextIds": [
            200590005
          ],
          "kind": "Player"
        },
        "200590005": {
          "type": "Sticker",
          "id": 200590005,
          "sectionId": 2005900,
          "senderId": null,
          "text": "[Thumbs Up]",
          "option": "No problem",
          "nextIds": [
            200590007
          ],
          "kind": "PlayerAuto",
          "sticker": {
            "id": 20006,
            "path": "SpriteOutput/Emoji/20006.png",
            "keywords": "Like"
          }
        },
        "200590007": {
          "type": "Text",
          "id": 200590007,
          "sectionId": 2005900,
          "senderId": null,
          "text": "But I haven't heard the story today, would I be able to understand anything tomorrow?",
          "option": null,
          "nextIds": [
            200590011
          ],
          "kind": "PlayerAuto"
        },
        "200590011": {
          "type": "Text",
          "id": 200590011,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "Oh the story today is about the Cloudstrider, and apparently they have a train",
          "option": null,
          "nextIds": [
            200590012
          ],
          "kind": "NPC"
        },
        "200590012": {
          "type": "Text",
          "id": 200590012,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "This train is like the Xianzhou ships and can travel between worlds",
          "option": null,
          "nextIds": [
            200590013
          ],
          "kind": "NPC"
        },
        "200590013": {
          "type": "Text",
          "id": 200590013,
          "sectionId": 2005900,
          "senderId": null,
          "text": "Why does that sound familiar...",
          "option": "It does sound familiar",
          "nextIds": [
            200590014
          ],
          "kind": "Player"
        },
        "200590014": {
          "type": "Text",
          "id": 200590014,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "Right?",
          "option": null,
          "nextIds": [
            200590015
          ],
          "kind": "NPC"
        },
        "200590015": {
          "type": "Text",
          "id": 200590015,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "That's why I'm asking for a favor",
          "option": null,
          "nextIds": [
            200590016
          ],
          "kind": "NPC"
        },
        "200590016": {
          "type": "Text",
          "id": 200590016,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "Mr. Xiyan said the Cloudstrider once arrived at a planet called Harilo",
          "option": null,
          "nextIds": [
            200590017
          ],
          "kind": "NPC"
        },
        "200590017": {
          "type": "Text",
          "id": 200590017,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "But was sucked into a huge conspiracy and even battled a massive aurumaton",
          "option": null,
          "nextIds": [
            200590018
          ],
          "kind": "NPC"
        },
        "200590018": {
          "type": "Text",
          "id": 200590018,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "Don't spoil whatever happens next to me, just tell me what Mr. Xiyan's version is tomorrow",
          "option": null,
          "nextIds": [
            200590019,
            200590020
          ],
          "kind": "NPC"
        },
        "200590019": {
          "type": "Text",
          "id": 200590019,
          "sectionId": 2005900,
          "senderId": null,
          "text": "Where is Harilo?",
          "option": "Where is Harilo?",
          "nextIds": [
            200590021
          ],
          "kind": "Player"
        },
        "200590021": {
          "type": "Text",
          "id": 200590021,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "...",
          "option": null,
          "nextIds": [
            200590022
          ],
          "kind": "NPC"
        },
        "200590022": {
          "type": "Text",
          "id": 200590022,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "So you don't know?",
          "option": null,
          "nextIds": [
            200590023
          ],
          "kind": "NPC"
        },
        "200590023": {
          "type": "Text",
          "id": 200590023,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "Alright, looks like I still have to be there in person tomorrow to get the story",
          "option": null,
          "nextIds": [
            200590024
          ],
          "kind": "NPC"
        },
        "200590024": {
          "type": "Text",
          "id": 200590024,
          "sectionId": 2005900,
          "senderId": null,
          "text": "I'll tell you my story next time",
          "option": "I'll tell you my story next time",
          "nextIds": [
            200590025
          ],
          "kind": "Player"
        },
        "200590025": {
          "type": "Text",
          "id": 200590025,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "Okay!!!",
          "option": null,
          "nextIds": [
            200590026
          ],
          "kind": "NPC"
        },
        "200590026": {
          "type": "Sticker",
          "id": 200590026,
          "sectionId": 2005900,
          "senderId": 1201,
          "text": "[Thumbs Up]",
          "option": null,
          "nextIds": [],
          "kind": "NPC",
          "sticker": {
            "id": 20006,
            "path": "SpriteOutput/Emoji/20006.png",
            "keywords": "Like"
          }
        },
        "200590020": {
          "type": "Text",
          "id": 200590020,
          "sectionId": 2005900,
          "senderId": null,
          "text": "What is this massive aurumaton?",
          "option": "What is this massive aurumaton?",
          "nextIds": [
            200590021
          ],
          "kind": "Player"
        },
        "200590009": {
          "type": "Text",
          "id": 200590009,
          "sectionId": 2005900,
          "senderId": null,
          "text": "I'm a little busy tomorrow",
          "option": "No time",
          "nextIds": [
            200590008
          ],
          "kind": "Player"
        },
        "200590008": {
          "type": "Sticker",
          "id": 200590008,
          "sectionId": 2005900,
          "senderId": null,
          "text": "[cry]",
          "option": "No time",
          "nextIds": [
            200590010
          ],
          "kind": "PlayerAuto",
          "sticker": {
            "id": 20005,
            "path": "SpriteOutput/Emoji/20005.png",
            "keywords": "Crying"
          }
        },
        "200590010": {
          "type": "Text",
          "id": 200590010,
          "sectionId": 2005900,
          "senderId": null,
          "text": "And I haven't heard the story today, it's likely I wouldn't be able to understand anything tomorrow",
          "option": null,
          "nextIds": [
            200590011
          ],
          "kind": "PlayerAuto"
        }
      }
    },
