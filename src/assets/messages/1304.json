        "messages": {
          "201330000": {
            "type": "Text",
            "id": 201330000,
            "sectionId": 2013300,
            "senderId": 1304,
            "text": "Hello, {F#Miss}{M#Mr.} Stellaron!",
            "option": null,
            "nextIds": [
              201330001,
              201330002
            ],
            "kind": "NPC"
          },
          "201330001": {
            "type": "Text",
            "id": 201330001,
            "sectionId": 2013300,
            "senderId": null,
            "text": "You! How!?",
            "option": "You! How!?",
            "nextIds": [
              201330003
            ],
            "kind": "Player"
          },
          "201330003": {
            "type": "Text",
            "id": 201330003,
            "sectionId": 2013300,
            "senderId": 1304,
            "text": "Such a pity I cannot read your response",
            "option": null,
            "nextIds": [
              201330004
            ],
            "kind": "NPC"
          },
          "201330004": {
            "type": "Text",
            "id": 201330004,
            "sectionId": 2013300,
            "senderId": 1304,
            "text": "This is a scheduled message",
            "option": null,
            "nextIds": [
              201330005
            ],
            "kind": "NPC"
          },
          "201330005": {
            "type": "Text",
            "id": 201330005,
            "sectionId": 2013300,
            "senderId": 1304,
            "text": "By the time this text arrives, our conflict should have come to an end",
            "option": null,
            "nextIds": [
              201330006
            ],
            "kind": "NPC"
          },
          "201330006": {
            "type": "Text",
            "id": 201330006,
            "sectionId": 2013300,
            "senderId": 1304,
            "text": "If all goes well, we may still have a chance to meet again",
            "option": null,
            "nextIds": [
              201330007
            ],
            "kind": "NPC"
          },
          "201330007": {
            "type": "Text",
            "id": 201330007,
            "sectionId": 2013300,
            "senderId": 1304,
            "text": "But who knows, sometimes I too have my share of setbacks",
            "option": null,
            "nextIds": [
              201330008
            ],
            "kind": "NPC"
          },
          "201330008": {
            "type": "Text",
            "id": 201330008,
            "sectionId": 2013300,
            "senderId": 1304,
            "text": "Let's say goodbye for now, friend. Aventurine sends you his heartfelt gratitude and utmost respect.",
            "option": null,
            "nextIds": [
              201330009
            ],
            "kind": "NPC"
          },
          "201330009": {
            "type": "Text",
            "id": 201330009,
            "sectionId": 2013300,
            "senderId": 1304,
            "text": "May joy reign over your journey",
            "option": null,
            "nextIds": [
              201330010,
              201330011,
              201330012
            ],
            "kind": "NPC"
          },
          "201330010": {
            "type": "Text",
            "id": 201330010,
            "sectionId": 2013300,
            "senderId": null,
            "text": "See you later. Hope to see you again soon",
            "option": "See you later",
            "nextIds": [
              201330013
            ],
            "kind": "Player"
          },
          "201330013": {
            "type": "Text",
            "id": 201330013,
            "sectionId": 2013300,
            "senderId": null,
            "text": "Message failed to send.",
            "option": null,
            "nextIds": [
              201330014
            ],
            "kind": "System"
          },
          "201330014": {
            "type": "Text",
            "id": 201330014,
            "sectionId": 2013300,
            "senderId": null,
            "text": "The user you're trying to reach is currently out of the service area. Please try again later.",
            "option": null,
            "nextIds": [],
            "kind": "System"
          },
          "201330011": {
            "type": "Text",
            "id": 201330011,
            "sectionId": 2013300,
            "senderId": null,
            "text": "You'll pay for what you did when you come back",
            "option": "You'll pay when you're back",
            "nextIds": [
              201330013
            ],
            "kind": "Player"
          },
          "201330012": {
            "type": "Text",
            "id": 201330012,
            "sectionId": 2013300,
            "senderId": null,
            "text": "Don't ever come back",
            "option": "Don't ever come back",
            "nextIds": [
              201330013
            ],
            "kind": "Player"
          },
          "201330002": {
            "type": "Text",
            "id": 201330002,
            "sectionId": 2013300,
            "senderId": null,
            "text": "You're still alive?",
            "option": "You're still alive?",
            "nextIds": [
              201330003
            ],
            "kind": "Player"
          }
        }
      }
    ],
    [
      {
