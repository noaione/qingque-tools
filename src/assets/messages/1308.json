          "201480001": {
            "type": "Text",
            "id": 201480001,
            "sectionId": 2014800,
            "senderId": null,
            "text": "Hello!",
            "option": "Hello!",
            "nextIds": [
              201480002
            ],
            "kind": "Player"
          },
          "201480002": {
            "type": "Text",
            "id": 201480002,
            "sectionId": 2014800,
            "senderId": 1308,
            "text": "Hello",
            "option": null,
            "nextIds": [
              201480003
            ],
            "kind": "NPC"
          },
          "201480003": {
            "type": "Text",
            "id": 201480003,
            "sectionId": 2014800,
            "senderId": 1308,
            "text": "It's been a while since I heard such a normal greeting, it's really comforting",
            "option": null,
            "nextIds": [
              201480007
            ],
            "kind": "NPC"
          },
          "201480007": {
            "type": "Text",
            "id": 201480007,
            "sectionId": 2014800,
            "senderId": 1308,
            "text": "Before coming to Penacony, I thought it would be just a regular trip",
            "option": null,
            "nextIds": [
              201480008
            ],
            "kind": "NPC"
          },
          "201480008": {
            "type": "Text",
            "id": 201480008,
            "sectionId": 2014800,
            "senderId": 1308,
            "text": "To fulfill a dead man's wish and take him back to a place he could never return to",
            "option": null,
            "nextIds": [
              201480009
            ],
            "kind": "NPC"
          },
          "201480009": {
            "type": "Text",
            "id": 201480009,
            "sectionId": 2014800,
            "senderId": 1308,
            "text": "But before I knew it, this world became so vibrant and colorful",
            "option": null,
            "nextIds": [
              201480010
            ],
            "kind": "NPC"
          },
          "201480010": {
            "type": "Text",
            "id": 201480010,
            "sectionId": 2014800,
            "senderId": 1308,
            "text": "And I've become a part of it",
            "option": null,
            "nextIds": [
              201480011,
              201480015
            ],
            "kind": "NPC"
          },
          "201480011": {
            "type": "Text",
            "id": 201480011,
            "sectionId": 2014800,
            "senderId": null,
            "text": "You played a crucial role!",
            "option": "You played a crucial role!",
            "nextIds": [
              201480012
            ],
            "kind": "Player"
          },
          "201480012": {
            "type": "Sticker",
            "id": 201480012,
            "sectionId": 2014800,
            "senderId": null,
            "text": "",
