          "200960006": {
            "type": "Text",
            "id": 200960006,
            "sectionId": 2009600,
            "senderId": 1112,
            "text": "Based on my observation, you've got some serious potential to join the Strategic Investment Department",
            "option": null,
            "nextIds": [
              200960007
            ],
            "kind": "NPC"
          },
          "200960007": {
            "type": "Text",
            "id": 200960007,
            "sectionId": 2009600,
            "senderId": 1112,
            "text": "The scale of our operations here at the IPC speaks for itself. With your skills, you could easily climb up to rank P<unbreak>20</unbreak> with just a few missions under your belt.",
            "option": null,
            "nextIds": [
              200960008
            ],
            "kind": "NPC"
          },
          "200960008": {
            "type": "Text",
            "id": 200960008,
            "sectionId": 2009600,
            "senderId": 1112,
            "text": "If I were to put in a good word for you, becoming a branch manager in <unbreak>10</unbreak> years is not impossible for you!",
