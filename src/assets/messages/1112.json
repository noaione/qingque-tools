            "nextIds": [
              200960012
            ],
            "kind": "NPC"
          },
          "200960012": {
            "type": "Text",
            "id": 200960012,
            "sectionId": 2009600,
            "senderId": 1112,
            "text": "Wanna try?",
            "option": null,
            "nextIds": [
              200960013
            ],
            "kind": "NPC"
          },
          "200960013": {
            "type": "Text",
            "id": 200960013,
            "sectionId": 2009600,
            "senderId": null,
            "text": "Even if I'm not an employee?",
            "option": "Even if I'm not an employee?",
            "nextIds": [
              200960014
            ],
            "kind": "Player"
          },
          "200960014": {
            "type": "Text",
            "id": 200960014,
            "sectionId": 2009600,
            "senderId": 1112,
            "text": "No worries. Put it on my tab",
            "option": null,
            "nextIds": [
              200960022
            ],
            "kind": "NPC"
          },
          "200960022": {
            "type": "Text",
            "id": 200960022,
            "sectionId": 2009600,
            "senderId": null,
            "text": "Are there any other perks for me?",
            "option": "Any other perks for me?",
            "nextIds": [
              200960023
