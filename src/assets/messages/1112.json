        "messages": {
          "200950000": {
            "type": "Text",
            "id": 200950000,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "The project is equipped with a massive protective barrier that can withstand even planetary impacts. It operates as a self-contained ecosystem, fulfilling the survival needs of entire Amber Eras. Under the IPC's protection, almost no external forces can affect this facility. Without a doubt, it's the safest sanctuary in the cosmos.",
            "option": null,
            "nextIds": [
              200950001,
              200950005
            ],
            "kind": "NPC"
          },
          "200950001": {
            "type": "Text",
            "id": 200950001,
            "sectionId": 2009500,
            "senderId": null,
            "text": "Why do people keep getting hacked?",
            "option": "Another hacked account?",
            "nextIds": [
              200950002
            ],
            "kind": "Player"
          },
          "200950002": {
            "type": "Text",
            "id": 200950002,
            "sectionId": 2009500,
            "senderId": null,
            "text": "All these weird messages...",
            "option": null,
            "nextIds": [
              200950003
            ],
            "kind": "PlayerAuto"
          },
          "200950003": {
            "type": "Text",
            "id": 200950003,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "This is a proposal I'm doing for a project",
            "option": null,
            "nextIds": [
              200950004
            ],
            "kind": "NPC"
          },
          "200950004": {
            "type": "Text",
            "id": 200950004,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "Interested?",
            "option": null,
            "nextIds": [
              200950009
            ],
            "kind": "NPC"
          },
          "200950009": {
            "type": "Text",
            "id": 200950009,
            "sectionId": 2009500,
            "senderId": null,
            "text": "Do you not rest?",
            "option": "Do you not need rest?",
            "nextIds": [
              200950010
            ],
            "kind": "Player"
          },
          "200950010": {
            "type": "Text",
            "id": 200950010,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "I *am* resting. Making proposals is relaxing for me",
            "option": null,
            "nextIds": [
              200950011
            ],
            "kind": "NPC"
          },
          "200950011": {
            "type": "Text",
            "id": 200950011,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "Do you like your work?",
            "option": null,
            "nextIds": [
              200950012,
              200950015,
              200950018
            ],
            "kind": "NPC"
          },
          "200950012": {
            "type": "Text",
            "id": 200950012,
            "sectionId": 2009500,
            "senderId": null,
            "text": "I love it. I can't even eat properly if I don't work",
            "option": "Love it",
            "nextIds": [
              200950013
            ],
            "kind": "Player"
          },
          "200950013": {
            "type": "Text",
            "id": 200950013,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "That's right. I knew you'd understand",
            "option": null,
            "nextIds": [
              200950014
            ],
            "kind": "NPC"
          },
          "200950014": {
            "type": "Text",
            "id": 200950014,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "You've gained a few more points in my eyes",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "200950015": {
            "type": "Text",
            "id": 200950015,
            "sectionId": 2009500,
            "senderId": null,
            "text": "I don't like it. Just thinking about work gives me a full-body migraine",
            "option": "I don't like it",
            "nextIds": [
              200950016
            ],
            "kind": "Player"
          },
          "200950016": {
            "type": "Text",
            "id": 200950016,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "Is it? That's unfortunate",
            "option": null,
            "nextIds": [
              200950017
            ],
            "kind": "NPC"
          },
          "200950017": {
            "type": "Text",
            "id": 200950017,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "If everyone could find purpose in life in their work, wouldn't it be nice?",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "200950018": {
            "type": "Text",
            "id": 200950018,
            "sectionId": 2009500,
            "senderId": null,
            "text": "I don't have a job",
            "option": "I don't have a job",
            "nextIds": [
              200950019
            ],
            "kind": "Player"
          },
          "200950019": {
            "type": "Text",
            "id": 200950019,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "Aren't you a Nameless?",
            "option": null,
            "nextIds": [
              200950020,
              200950021
            ],
            "kind": "NPC"
          },
          "200950020": {
            "type": "Text",
            "id": 200950020,
            "sectionId": 2009500,
            "senderId": null,
            "text": "The Express doesn't pay me a salary",
            "option": "The Express doesn't pay me a salary",
            "nextIds": [
              200950022
            ],
            "kind": "Player"
          },
          "200950022": {
            "type": "Text",
            "id": 200950022,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "I see...",
            "option": null,
            "nextIds": [
              200950023
            ],
            "kind": "NPC"
          },
          "200950023": {
            "type": "Text",
            "id": 200950023,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "Looks like you've also gone past the stage of working just for money",
            "option": null,
            "nextIds": [
              200950024
            ],
            "kind": "NPC"
          },
          "200950024": {
            "type": "Text",
            "id": 200950024,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "You've gained a few more points in my eyes",
            "option": null,
            "nextIds": [],
            "kind": "NPC"
          },
          "200950021": {
            "type": "Text",
            "id": 200950021,
            "sectionId": 2009500,
            "senderId": null,
            "text": "I work for love!",
            "option": "I work for love!",
            "nextIds": [
              200950022
            ],
            "kind": "Player"
          },
          "200950005": {
            "type": "Text",
            "id": 200950005,
            "sectionId": 2009500,
            "senderId": null,
            "text": "How much to invest? Count me in!",
            "option": "How much? I'm in",
            "nextIds": [
              200950006
            ],
            "kind": "Player"
          },
          "200950006": {
            "type": "Text",
            "id": 200950006,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "Aye, someone's being decisive!",
            "option": null,
            "nextIds": [
              200950007
            ],
            "kind": "NPC"
          },
          "200950007": {
            "type": "Text",
            "id": 200950007,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "This is a proposal I'm doing for a project",
            "option": null,
            "nextIds": [
              200950008
            ],
            "kind": "NPC"
          },
          "200950008": {
            "type": "Text",
            "id": 200950008,
            "sectionId": 2009500,
            "senderId": 1112,
            "text": "I knew you'd be interested",
            "option": null,
            "nextIds": [
              200950009
            ],
            "kind": "NPC"
          }
        }
      }
