    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.19.2",
    "npm-run-all2": "6.1.1",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "tailwindcss": "3.4.0",
    "typescript": "~5.2.0",
    "unplugin-auto-import": "0.17.2",
    "unplugin-icons": "0.18.1",
    "unplugin-vue-components": "0.26.0",
