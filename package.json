{
  "name": "qingque-scoring-v2",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run build && npm run preview",
    "dev": "vite",
    "pre-build": "node scripts/pre-build-hook.js",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
